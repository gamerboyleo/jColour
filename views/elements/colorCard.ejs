<a onClick="toggleModal('<%= element.name %>', '<%= element.hexColor %>')" class="card is-colour is-fullwidth-mobile" style="background-color: <%=element.hexColor%>">
    <div class="card-content">
        <h2 id="<%= element.id %>" style="color: <%= tinycolor(element.hexColor).isDark() ? "white" : "black" %>" class="title is-5 style-colour">
            <%= element.name.substring(7) %>
        </h2>
    </div>
</a>